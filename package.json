{
  "name": "wallet-api-be",
  "version": "0.0.1",
  "description": "",
  "author": "Skillful Dev LLC",
  "private": true,
  "license": "Liber Menna",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build -b swc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start -b swc --type-check",
    "start:dev": "nest start --watch  -b swc --type-check",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --maxWorkers=50%",
    "test-github": "jest --maxWorkers=50% --silent",
    "test-s": "jest --maxWorkers=50% --silent",
    "test:v": "jest --maxWorkers=50% --verbose",
    "test:single-thread": "jest --runInBand",
    "test:cov": "jest --coverage --maxWorkers=50%",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "compodoc": "npx @compodoc/compodoc -p tsconfig.json -s",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -f src/config/database.ts",
    "migration:create": "npm run typeorm:cli -- migration:create -d src/migrations -n",
    "migration:run": "npm run typeorm:cli -- migration:run",
    "migration:revert": "npm run typeorm:cli -- migration:revert"
  },
  "dependencies": {
    "@ankr.com/ankr.js": "^0.4.2",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.1.1",
    "@nestjs/core": "^10.1.1",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.1.1",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.2",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.1.1",
    "@vendia/serverless-express": "^4.10.4",
    "aws-lambda": "^1.0.7",
    "bcrypt": "5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.3.1",
    "helmet": "^7.0.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "nest-winston": "^1.9.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.1",
    "pg-mem": "^2.6.13",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "siwe": "^2.1.4",
    "swagger-ui-express": "^5.0.0",
    "terser-webpack-plugin": "^5.3.9",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.21",
    "@nestjs/cli": "^10.1.10",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.1.1",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.70",
    "@types/aws-lambda": "^8.10.123",
    "@types/bcrypt": "^5.0.0",
    "@types/bluebird": "^3.5.38",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.3",
    "@types/lodash": "^4.14.195",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.4",
    "@types/passport-jwt": "^3.0.9",
    "@types/sharp": "^0.31.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "29.6.1",
    "lint-staged": "^13.2.3",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-offline": "^13.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.ts"
    ],
    "coveragePathIgnorePatterns": [
      ".dto.ts",
      ".decorator.ts",
      ".catalogs.ts",
      ".middleware.ts",
      ".filter.ts",
      ".module.ts",
      "src/errors",
      "src/config",
      "main.ts"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}